                <a href="" id="twitter" class="social-links">
                    <img src="./images/icons/twitter.png" alt="">
                </a>
            </div>
        </div>
        <div id="theme">
            <div id="theme-btn">
                <img id="theme-img" src="./images/icons/color-wheel.png" alt="">
            </div>

            <div id="theme-clr">
                <div id="white-orange" class="color-theme">
                </div>
                <div id="white-green" class="color-theme">
                </div>
                <div id="white-pink" class="color-theme">
                </div>
                <div id="grey-black" class="color-theme">  
